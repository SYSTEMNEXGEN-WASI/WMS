<script>
    //var GlobalVar_CusCode = null;
    //var GlobalVar_AccountOf = null;

    //For Search in Model->START
    $(document).ready(function () {
        $("#txtEnquirySearch").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#InsCompTable tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });

    });
    //For Search in Model->End

    //For Numeric and Decimal Values Only In Text Box   ->START
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode != 46 && charCode > 31
          && (charCode < 48 || charCode > 57))
            return false;
        if (charCode == 46 && evt.srcElement.value.split('.').length > 1) { return false; }

        return true;
    }
    //For Numeric and Decimal Values Only In Text Box   ->END


    //For Session Buttons->START
    $(document).ready(function () {
        var dealer = 'COMON';
        Get_InsuranceCom(dealer);

        document.getElementById("ddlAllDealer").style.display = "none";
        $('input:radio[name=Sessions]').change(function () {
            if (this.value == '@Session["DealerCode"].ToString()') {
                var dealer = '@Session["DealerCode"].ToString()';
                $("[name='ddlAllDealer']").hide();
                Get_InsuranceCom(dealer);
            }
            else if (this.value == 'COMON') {
                $("[name='ddlAllDealer']").hide();
                var dealer = 'COMON';
                Get_InsuranceCom(dealer);

            }
            else if (this.value == 'Dealer') {
                var dealer = $("#ddlAllDealer").val();
                $("[name='ddlAllDealer']").show();
                Get_InsuranceCom(dealer);

            }
        });
    });
    $("#ddlAllDealer").change(function () {

        var val = $("#ddlAllDealer").val();
        Get_InsuranceCom(val);
    });//For Session Buttons->End

    function ShowModal(type) {
        if (type == 'Jobs') {
            $('#JobModal').modal("show");
        } else
            if (type == 'accountof') {
                $('#AccountOfModal').modal("show");
            }
        if (type == 'InvoiceReq') {
            $('#InvoiceReqModal').modal("show");
        }
    }

    function Get_InsuranceCom(value) {
        var rowCount = 1;
        var dataCount = 1;
        var SNo = 1;
        $.ajax({
            dataType: "json",
            async: true,
            type: 'GET',
            data: { 'dealer': value },
            //Select_BrandDetails
            url: '@Url.Content("~/InsuranceCompany/Select_InsuranceCompany")',
            success: function (data) {

                if (data.Success == true) {

                    var item = JSON.parse(data.Response)
                    $("#InsCompTable tbody tr").remove()

                    if (item.length > 0) {

                        $.each(item, function (value, item) {

                            $("#InsCompTable tbody").append('<tr onclick="Get_InsComp(this);" id="InsCompFixedRow' + (rowCount++) + '" data-tr-count="' + (dataCount++) + '">' +
                                           '<td>' + SNo++ + '</td>' +
                                           '<td id="lblDealerCode">' + item.DealerCode + '<input type="hidden" id="hiddenlblInsCompPhone1" value="' + item.InsCompPhone1 + '"/><input type="hidden" id="hiddenlblInsCompEmail" value="' + item.InsCompEmail + '"/><input type="hidden" id="hiddenlblInsCompAddress1" value="' + item.InsCompAddress1 + '"/><input type="hidden" id="hiddenlblInsCompFax" value="' + item.InsCompFax + '"/></td>' +
                                           '<td id="lblInsComCode">' + item.InsCompCode + '<input type="hidden" id="hiddenlblInsCompCellNo" value="' + item.InsCompCellNo + '"/><input type="hidden" id="hiddenlblInsCompContactPerson" value="' + item.InsCompContactPerson + '"/><input type="hidden" id="hiddenlblInsCompURL" value="' + item.InsCompURL + '"/><input type="hidden" id="hiddenlblInsCompCellNo" value="' + item.InsCompCellNo + '"/></td>' +
                                           '<td id="lblInsCompDescription">' + item.InsCompDescription + '<input type="hidden" id="hiddenlblInsCompStndrdDiscount" value="' + item.InsCompStndrdDiscount + '"/><input type="hidden" id="hiddenlblInsCompContactPerson" value="' + item.InsCompContactPerson + '"/><input type="hidden" id="hiddenlblSalesTaxRegNo" value="' + item.SalesTaxRegNo + '"/><input type="hidden" id="hiddenlblNTNNo" value="' + item.NTNNo + '"/></td>' +

                                          '<td style="text-align:center;">' +
                                          '<a id="btnEdit" class="selectItem"><i class="fa fa-edit" style="color:blue"></i></a> ' +
                                          '</td>' +


                                               '</tr>');
                        });

                    }
                }
            },
            complete: function () {

            },
        });
    }

    function Get_InsComp(value) {

        var phone = $(value).closest('tr').find("#hiddenlblInsCompPhone1").val();
        var email = $(value).closest('tr').find("#hiddenlblInsCompEmail").val();
        var contactperson = $(value).closest('tr').find("#hiddenlblInsCompContactPerson").val();
        var address = $(value).closest('tr').find("#hiddenlblInsCompAddress1").val();
        var url = $(value).closest('tr').find("#hiddenlblInsCompURL").val();
        var discount = $(value).closest('tr').find("#hiddenlblInsCompStndrdDiscount").val();
        var salestax = $(value).closest('tr').find("#hiddenlblSalesTaxRegNo").val();
        var ntn = $(value).closest('tr').find("#hiddenlblNTNNo").val();
        var Cell = $(value).closest('tr').find("#hiddenlblInsCompCellNo").val();
        var fax = $(value).closest('tr').find("#hiddenlblInsCompFax").val();
        var InsCompCode = $(value).closest('tr').find("#lblInsComCode").text().trim();
        var InsCompDesc = $(value).closest('tr').find("#lblInsCompDescription").text().trim();

        //var CusCode = $(value).closest('tr').find("#hiddenlblCustomerCode").val
        @*var dealerCode = "@Session["DealerCode"].ToString()";*@



        $("#txtInsComCode").val(InsCompCode);
        $("#txtInsComDesc").val(InsCompDesc);
        $("#txtInsComPhone").val(phone);
        $("#txtInsComEmail").val(email);
        $("#txtInsComContact").val(contactperson);
        $("#txtInsComURL").val(url);
        $("#txtInsComDiscount").val(discount);
        $("#txtInsComSalesTax").val(salestax);
        $("#txtInsComNTN").val(ntn);
        $("#txtInsComCell").val(Cell);
        $("#txtInsComFax").val(fax);
        $("#txtInsComAddress").val(address);

        $('#JobModal').modal("hide");
    }


