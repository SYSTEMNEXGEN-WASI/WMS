
@{
    ViewBag.Title = "ItemMaster";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}


<link href='@Url.Content("~/assets/plugins/select2/select2.css")' rel="stylesheet" type="text/css" media="screen" />
<link href='@Url.Content("~/assets/plugins/icheck/skins/minimal/blue.css")' rel="stylesheet" type="text/css" media="screen" />
<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/assets/plugins/select2/select2.min.js")' type="text/javascript"></script>
<script src="../assets/plugins/icheck/icheck.min.js" type="text/javascript"></script>
<script src="~/assets/js/scrollablegridplugin_asp.netajax_2.0.js"></script>
<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/assets/plugins/jquery-ui/smoothness/jquery-ui.min.js")' type="text/javascript"></script>

<h2>ItemMaster</h2>
<style>
    .makespace {
        margin-bottom: 20px;
    }

    .icheckbox_minimal-blue {
        top: 10px;
    }
</style>
<section class="wrapper main-wrapper row">
    <form method="post">
        <div class="col-xs-12">
            <section class="box">
                <header class="panel_header">
                    <h2 class="title pull-left">Item Master</h2>
                    <div class="actions panel_actions pull-right">
                        <a class="box_toggle fa fa-chevron-down"></a>
                    </div>

                </header>
                <div class="content-body">
                
                    <div class="row">
                        <div class="col-md-4">
                            <label>Item Code</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('Jobs')" style="color:red;"></i></span>
                                <input type="text" class="form-control" id="txtItemCode" readonly="readonly">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                            <label>Part Item No.</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                <input type="text" class="form-control" id="txtPartItemNo">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                            <label>Item Discription</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                <input type="text" class="form-control" id="txtItemDesc"  maxlength="150">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4" style="margin-top:10px">
                            <label>Vehicle Category</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                <select class="form-control m-bot15" id="ddlVehicle">
                                    <option value="0">Select</option>
                                    <option value="LCV">LCV</option>
                                    <option value="HCV">HCV</option>
                                </select>
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <!--Radio Box-->
                        <div class="row ; col-md-4">
                            <div class="box" style="padding:2px 5px 2px 20px;">
                                <div>
                                    <label>
                                        <input type="radio" class="chkCurrentUser" name="Sessions" id="chkCurrentUser" value="@Session["DealerCode"].ToString()">
                                        <label id="lblcu" value="@Session["DealerCode"].ToString()" class="form-label">Current User</label>
                                    </label>
                                </div>
                                <div>
                                    <label>
                                        <input type="radio" class="chkForAll" name="Sessions" checked id="chkForAll" value="COMON">
                                        <label id="lblForAll" value="COMON" class="form-label">For All</label>
                                    </label>
                                </div>
                                <div>
                                    <label>
                                        <input type="radio" class="chkDealer" name="Sessions" id="chkDealer" value="Dealer">
                                        <label id="lblDealer" value="Dealer" style="" class="form-label"> Dealer </label>
                                        @Html.DropDownList("ddlAllDealer", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlAllDealer, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlAllDealer" })
                                    </label>
                                </div>
                            </div>
                            <div class="makespace"></div>
                        </div>


                        <!--next-->
                        <div class="col-md-4" hidden>
                            <label>Labour A/C</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-male"></i></span>
                                <input type="text" class="form-control" id="txtJobLabour" maxlength="150">

                            </div>
                            <div class="makespace"></div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:20px">
                        <ul class="nav nav-tabs nav-justified primary">
                            <li class="active">
                                <a href="#general" data-toggle="tab">
                                    <i></i> General
                                </a>
                            </li>
                            <li>
                                <a href="#inventory" data-toggle="tab">
                                    <i></i> Inventory
                                </a>
                            </li>
                            <li>
                                <a href="#sales" data-toggle="tab">
                                    <i></i> Sales
                                </a>
                            </li>
                            <li>
                                <a href="#purchase" data-toggle="tab">
                                    <i></i> Purchase
                                </a>
                            </li>

                        </ul>

                        <div class="tab-content primary">
                            <div class="tab-pane fade in active" id="general">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label>Item Category</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            @Html.DropDownList("ddlItemCatDesc", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlItemCatDesc, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlItemCatDesc" })
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                        <label>Product Code</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('ProductCode')" style="color:red;"></i></span>
                                            <input type="text" class="form-control" id="txtProductCode" readonly>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                        <label>Unit</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtUnitDesc" readonly>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4" style="margin-top:10px">
                                        <label>Pack Size</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtPackSize">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4" style="margin-top:10px">
                                            <label>Part Type</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                                <select class="form-control m-bot15" id="ddlPartType">
                                                    <option value="0">Select</option>
                                                    <option value="Y">Lube</option>
                                                    <option value="N">Spare</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    <div class="col-md-4" style="margin-top:10px">
                                            <label>Liter</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                <input type="text" class="form-control" id="txtLiter" readonly maxlength="5">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    <div class="col-md-4" >
                                            <label>Accessories</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                                <select class="form-control m-bot15" id="ddlAccessories">
                                                    <option value="0">Select</option>
                                                    <option value="Y">Yes</option>
                                                    <option value="N">No</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    <div class="col-md-4">
                                        <label>Unit of Sold</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtUnitOfSold">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    
                                        
                                    <div class="col-md-4">
                                            <label>Source</label>
                                            <div class="box" style="height:60px">
                                                    <label class="col-md-3">
                                                        <input type="radio" class="chkCurrentUser" name="CheckBox" id="chkCU" value="ARC">
                                                        <label id="lblcu" value="@Session["DealerCode"].ToString()">Local</label>
                                                    </label>
                                                    <label class="col-md-3">
                                                        <input type="radio" class="chkForAll" name="CheckBox" checked id="chkForAll" value="IRC">
                                                        <label id="lblForAll" value="COMON">C.K.D.</label>
                                                    </label>
                                                    <label class="col-md-3">
                                                        <input type="radio" class="chkDealer" name="CheckBox" id="chkDealer" value="RCWOI">
                                                        <label id="lblDealer" value="Dealer"> Imported </label>
                                                    </label>
                                                    <label class="col-md-3">
                                                        <input type="radio" class="chkDealer" name="CheckBox" id="" value="RCWOI">
                                                        <label id="lblDealer" value="Dealer"> Market </label>
                                                    </label>
                                            </div>
                                        
                                        </div>
                                    <div class="col-md-5" style="margin-top:10px">
                                        <div class="box_toggle" style="height:72px;">

                                            <label class="col-md-4">
                                                <input id="chksaleitem" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label for="ctl00_Content_cbinventoryItem">Sale Item</label>
                                                
                                            </label>
                                            <label class="col-md-6">
                                                <input id="chkinventoryitem" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label for="ctl00_Content_cbinventoryItem">Inventory Item</label>
                                                
                                            </label>
                                            <label class="col-md-4">
                                                <input id="chkactive" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label for="ctl00_Content_cbinventoryItem">Active</label>
                                                
                                            </label>
                                            
                                        </div>

                                    </div>

                                    </div>
                            </div>

                            <div class="tab-pane fade" id="inventory">
                                <div class="row">
                                    
                                    
                                        <div class="col-md-4">
                                            <label>Min Level</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                <input type="text" class="form-control" id="txtMinLevel">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label>Reorder Level</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtReorderLevel">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label>War</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtWar">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        
                                        
                                        <div class="row" style="margin-top:1rem;" hidden>
                                            <div class="col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-bordered table-striped" id="WLabor" style="white-space: nowrap; FONT-SIZE: 13px;">
                                                        <thead>
                                                            <tr>
                                                                <th>S No</th>
                                                                <th>Operational Code</th>
                                                                <th>Op Description</th>
                                                                <th>Hr</th>
                                                                <th>Rate</th>
                                                                <th>Total</th>
                                                                <th>PST</th>
                                                                <th>PST Amount</th>
                                                                <th>AHFM Rate</th>
                                                                <th>AHFM Total</th>
                                                                <th>NetAmount</th>
                                                                <th>Action</th>
                                                                <th>Remove</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr id="fixedRow1" data-tr-count="0"></tr>
                                                            <tr>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-md-4" hidden>
                                            <label class="form-label">Total</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-clipboard"></i></span>
                                                <input type="text" class="form-control" id="txtLaborTotal" readonly />


                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                  
                                        <div class="col-md-4" hidden>
                                        <label class="form-label">Total</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-clipboard"></i></span>
                                            <input type="text" class="form-control" id="txtLaborTotal" readonly />


                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                               
                            </div>
                            </div>
                            <div class="tab-pane fade" id="sales">

                                <div class="row" style="margin-top:1rem;">
                                    <div class="col-md-12" hidden>
                                        <div class="table-responsive">
                                            <table class="table table-bordered table-striped" id="WRSublet" style="white-space: nowrap; FONT-SIZE: 13px;">
                                                <thead>
                                                    <tr>
                                                        <th>S No</th>
                                                        <th>Job</th>
                                                        <th>Sublet Date</th>
                                                        <th>Invoice Amount</th>
                                                        <th>PST Rate</th>
                                                        <th>PST Amount</th>
                                                        <th>AHFM Rate</th>
                                                        <th>NetAmount</th>
                                                        <th>Action</th>
                                                        <th>Remove</th>

                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr id="fixedRowSub" data-tr-count="0"></tr>
                                                    <tr>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="col-md-4">
                                            <label>Sales Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                <input type="text" class="form-control" id="txtSalesAmount">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label>Promotion Prod</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtPromotionProdSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label>Revenue A/C</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtRevenue">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4" style="margin-top:10px">
                                            <label>GST%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtGstSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4" style="margin-top:10px">
                                            <label>Further Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtFurtherTaxSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4" style="margin-top:10px">
                                            <label>Extra Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtExtraTaxSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4" style="margin-top:10px">
                                            <label>Stock Out A/C</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search" style="color:red;"></i></span>
                                                <input type="text" class="form-control" id="txtStockOut">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="tab-pane fade" id="purchase">

                                <div class="row" hidden>
                                    <div class="col-md-3">
                                        <label class="form-label">Approval No</label>
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="fa fa-clipboard"></i>
                                            </span>
                                            <input type="text" class="form-control" id="txtApprovalNo">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-3">
                                        <label class="form-label">Approval Date</label>
                                        <div class="input-group">
