<section class="wrapper main-wrapper row" style=''>

    <div class="clearfix"></div>
    <form method="post">
        <div class="col-xs-12">
            <section class="box">
                <header class="panel_header">
                    <h2 class="title pull-left">VehicleStockReport</h2>                   
                </header>
                <div class="content-body">                    
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right">
                                <a class="btn btn-primary" id="btnPrint" style="margin-top:25px;" href="#">Print</a>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </form>

</section>

<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>


<script>
       
    $("#btnPrint").click(function () {
        debugger;
        var baseurl = '@Url.Action("Export")';
        var url = baseurl;
        //window.location.href = url;
        window.open(url, '_blank');

    });
</script>