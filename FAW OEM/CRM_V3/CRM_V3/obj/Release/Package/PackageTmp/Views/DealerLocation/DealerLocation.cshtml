            $("#txtPDealerDesc").css("border-color", "red");
            $("#warning").text("Record Should Be Selected");
            $("#modalMessage").css("background-color", "#e46457");
            $("#section-message").modal('show');
            return;
        } else { $("#txtPDealerDesc").css("border-color", "#ccc"); }

        if (DealerCode == "" || DealerCode == null) {
            $("#txtDealerCode").css("border-color", "red");
            $("#warning").text("Record Should Be Selected");
            $("#modalMessage").css("background-color", "#e46457");
            $("#section-message").modal('show');
            return;
        } else { $("#txtDealerCode").css("border-color", "#ccc"); }

        if (DealerDesc == "" || DealerDesc == null) {
            $("#txtDealershipName").css("border-color", "red");
            $("#warning").text("Record Should Be Selected");
            $("#modalMessage").css("background-color", "#e46457");
            $("#section-message").modal('show');
            return;
        } else { $("#txtDealershipName").css("border-color", "#ccc"); }


        $("#section-confirm-Delete").modal("show");
    })
    function DeleteMasterEnquiry() {
        $("#section-confirm-Delete").modal("hide");

        var dto = {};

        dto.CompanyCode = $("#txtCompCode").val();
        dto.PDealerCode = $("#txtPDealerCode").val();
        dto.DealerCode = $("#txtDealerCode").val();

        $.ajax({
            dataType: "json",
            async: true,
            type: 'GET',
            url: '@Url.Content("~/DealerLocation/Delete_DealerLocation")',
            data: dto,
            success: function (data) {

                if (data) {
                    if (data.Success == true) {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#64c344");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                    }
                    else {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#e46457");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                    }
                }
            },
            complete: function () {

            },
            error: function (data) {
                $("#section-confirm").modal("hide")
                $("#modalMessage").css("background-color", "#e46457");
                $("#lblMessage").text(data.Message)
                $("#section-message").modal('show');
            }
        });
    }




</script>
