                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="modal123Label">Tax</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group row">
                        <label for="exampleInputuname" class="col-md-2">Search :</label>
                        <div class="col-md-10">
                            <input type="text" id="txtEnquirySearch" class="form-control" aria-controls="EnquiryTable" placeholder="Search . . . ">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="table-responsive table-responsive table-scroll" style="height:300px;">
                            <table id="JobTable" class="table table-bordered table-striped table-hover responsive" style="white-space: nowrap; FONT-SIZE: 13px;">
                                <thead>
                                    <tr>
                                        <th>Sr</th>
                                        <th>Dealer Code</th>
                                        <th>Tax Code</th>
                                        <th>Tax Description</th>
                                        <th>Changable</th>
                                        <th>Applicable</th>
                                        <th>Part</th>
