                                    <div class="col-md-4">
                                        <label class="form-label">Tax Type</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-file"></i></span>
                                            @Html.DropDownList("ddlTaxType", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlTaxType, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlTaxType" })
