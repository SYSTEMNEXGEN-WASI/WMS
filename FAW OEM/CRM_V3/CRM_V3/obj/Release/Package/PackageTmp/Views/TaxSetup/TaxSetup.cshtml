                                        <div class="makespace"></div>
                                    </div>

                                    <div class="col-md-8" style="padding:30px 0px 0px 16px;">
                                        <div class="row">
                                            <div>
                                                <div class="col-md-2">
                                                    <label class="form-label"><input type="checkbox" id="SubchkBox" />Sublet</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <label class="form-label"><input type="checkbox" id="BougchkBox" />Boughtout</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <label class="form-label"><input type="checkbox" id="LubchkBox" />Lubricant</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <label class="form-label"><input type="checkbox" checked id="PchkBox" name="PGFE" value="Y" />Part</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <label class="form-label"><input type="checkbox" checked id="LaborchkBox" name="LPST" value="Y" />Labour</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>

                                    <!--Part Check Box-->
                                    <div class="col-md-12">

                                        <div class="col-md-2">
                                            <label class="form-label">GST</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                <input type="text" class="form-control" id="txtGST" maxlength="8" onkeypress="return isNumberKey(event)">

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Further</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                <input type="text" class="form-control" id="txtFurther" maxlength="8" onkeypress="return isNumberKey(event)">

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Extra</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                <input type="text" class="form-control" id="txtExtra" maxlength="8" onkeypress="return isNumberKey(event)">

                                            </div>
                                            <div class="makespace"></div>
                                        </div>

                                        <div class="col-md-2">
                                            <label class="form-label">PST</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                <input type="text" class="form-control" id="txtPST" maxlength="8" onkeypress="return isNumberKey(event)">

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    </div>



                                </div>
                            </div>


                        </div>
                            </div>

                    </div>




                       

                        <!--Grid-->

                    <div class="row" style="margin-top:30px">
                        <div class="col-md-12 ">
                            <div class="row">
                                <div class="box">
                                    <div class="table-responsive table-responsive-border table-scroll">
                                        <table class="table table-bordered table-striped table-hover responsive" id="TaxSetupGrid" style="white-space: nowrap; FONT-SIZE: 13px;">
                                            <thead>
                                                <tr>
                                                    <th>Sr</th>
                                                    <th>Dealer Code</th>
                                                    <th>Tax Code</th>
                                                    <th>Tax Description</th>
                                                    <th>Changable</th>
                                                    <th>Applicable</th>
                                                    <th>Part</th>
                                                    <th>Sublet</th>
                                                    <th>Labor</th>
                                                    <th>BoughtOut</th>
                                                    <th>TaxType</th>
                                                    <th>GST</th>
                                                    <th>FurTax</th>
                                                    <th>ExtraTax</th>
                                                    <th>PST</th>
                                                    <th>Lubricant</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody style="font-size:12px; text-transform: uppercase">
                                                <tr id="TaxSetupGridRow"></tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="makespace"></div>
                        </div>
                    </div>
                    <!--Save/Clear/Delete-->
                    <div class="row">
                        <!--SAVE/Clear/Delete BUTTONS-->
                        <div class="col-md-8 pull-right" style="margin-top:27px;">
                           
                            
                            <div class="makespace"></div>
                        </div>
                    </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="pull-right">
                                    <button type="button" class="btn btn-success" id="btnSave" style="margin-right:23px;">
                                        Save
                                    </button>
                                    <button type="reset" class="btn btn-primary" id="btnclr" onclick="ClearDesc()" style="margin-right: 23px;">
                                        Clear
                                    </button>

                                    <button type="button" class="btn btn-danger" id="btnDelete" style="margin-right:23px;" disabled>
                                        Delete
                                    </button>
                                    <a class="btn btn-primary" id="btnPrint" style="margin-right:23px;"  href="#">Print</a>
                                    
                                </div>
                            </div>
                        </div>
                 

                </div>
            </section>

        </div>
    </form>






    <!-- Popup box modal start -->
    <div class="modal" id="section-confirm" tabindex="-1" role="dialog" aria-hidden="true">
