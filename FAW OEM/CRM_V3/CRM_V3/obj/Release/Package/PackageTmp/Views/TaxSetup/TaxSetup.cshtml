    function SaveMasterEnquiry(){
        var cu = CheckSession();
        var chkChange = document.getElementById('ChangechkBox').checked;;
        var chkApp = document.getElementById('AppchkBox').checked;;
        var chkSub = document.getElementById('SubchkBox').checked;;
        var chkbou = document.getElementById('BougchkBox').checked;;
        var chkLub = document.getElementById('LubchkBox').checked;;
        var chkPart = document.getElementById('PchkBox').checked;;
        var chkLabor = document.getElementById('LaborchkBox').checked;

        var dto = {};

        if (chkChange) {
            dto.ChangeAble = 'Y';
        } else {
            dto.ChangeAble = 'N';
        }

        if (chkApp) {
            dto.Applicable = 'Y';
        } else {
            dto.Applicable = 'N';
        }

        if (chkSub) {
            dto.Sublet = 'Y';
        } else {
            dto.Sublet = 'N';
        }

        if (chkbou) {
            dto.BoughtOut = 'Y';
        } else {
            dto.BoughtOut = 'N';
        }

        if (chkLub) {
            dto.Lubricant = 'Y';
        } else {
            dto.Lubricant = 'N';
        }

        if (chkPart) {
            dto.Part = 'Y';
        } else {
            dto.Part = 'N';
        }

        if (chkLabor) {
            dto.Labor = 'Y';
        } else {
            dto.Labor = 'N';
        }


        dto.DealerCode =cu;
        dto.TaxAppCode = $("#txtTaxCode").val();
        dto.TaxAppDesc = $("#txtTaxDesc").val();
        dto.TaxType = $("#ddlTaxType").val();
        dto.GST = $("#txtGST").val();
        dto.FurTax = $("#txtFurther").val();
        dto.ExtraTax = $("#txtExtra").val();
        dto.PST = $("#txtPST").val();


        $.ajax({
            dataType: "json",
            async: true,
            type: 'GET',
            url: '@Url.Content("~/TaxSetup/Insert_TaxSetup")',
            data: dto,
            success: function (data) {

                if (data) {
                    if (data.Success == true) {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#64c344");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                    }
                    else {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#e46457");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                    }
                }
            },
            complete: function () {

            },
            error: function (data) {
                $("#section-confirm").modal("hide")
                $("#modalMessage").css("background-color", "#e46457");
                $("#lblMessage").text(data.Message)
                $("#section-message").modal('show');
            }
        });

    }

