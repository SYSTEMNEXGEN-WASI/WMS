
@{
    ViewBag.Title = "ItemMaster";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

@*<h2>NEW</h2>*@


<link href='@Url.Content("~/assets/plugins/select2/select2.css")' rel="stylesheet" type="text/css" media="screen" />
<link href='@Url.Content("~/assets/plugins/icheck/skins/minimal/blue.css")' rel="stylesheet" type="text/css" media="screen" />
<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/assets/plugins/select2/select2.min.js")' type="text/javascript"></script>
<script src="../assets/plugins/icheck/icheck.min.js" type="text/javascript"></script>
<script src="~/assets/js/scrollablegridplugin_asp.netajax_2.0.js"></script>
@*<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>*@
<script src='@Url.Content("~/assets/plugins/jquery-ui/smoothness/jquery-ui.min.js")' type="text/javascript"></script>

@* Data Tables *@

<script src="~/assets/js/jquery-1.11.2.min.js"></script>
<link href="~/assets/plugins/datatables/css/jquery.dataTables.css" rel="stylesheet" />
<link href="~/assets/plugins/datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/assets/plugins/datatables/js/jquery.dataTables.min.js"></script>
<style>
    .makespace {
        margin-bottom: 15px;
    }

    .icheckbox_minimal-blue {
        top: 10px;
    }
</style>
<section class="wrapper main-wrapper row">
    <form method="post">
        <div class="col-xs-12">
            <section class="box">
                <header class="panel_header">
                    <h2 class="title pull-left">Item Master</h2>
                    <div class="actions panel_actions pull-right">
                        <a class="box_toggle fa fa-chevron-down"></a>
                    </div>

                </header>
                <div class="content-body">

                    <div class="row">
                        <div class="col-md-4">
                            <label class="form-label">Item Code</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('Jobs')" style="color:blue;"></i></span>
                                <input type="text" class="form-control" id="txtItemCode" readonly="readonly">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                             <label class="form-label">Part Item No.</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                <input type="text" class="form-control" id="txtPartItemNo" style="text-transform:uppercase">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                             <label class="form-label">Item Discription</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                <input type="text" class="form-control" id="txtItemDesc" maxlength="150" style="text-transform:uppercase">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4" style="margin-top:10px">
                             <label class="form-label">Vehicle Category</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                <select class="form-control m-bot15" id="ddlVehicle">
                                    <option value="0">Select</option>
                                    <option value="LCV">LCV</option>
                                    <option value="HCV">HCV</option>
                                </select>
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <!--Radio Box-->
                        <div class="col-md-6">
                            <div class="box" style="padding:31px 5px 2px 5px;">
                                <div>
                                     <label class="form-label">
                                        <input type="radio" class="chkCurrentUser" name="Sessions" id="chkCurrentUser" value="@Session["DealerCode"].ToString()">
                                        <label id="lblcu" value="@Session["DealerCode"].ToString()" class="form-label">Login User</label>
                                    </label>
                                    <label style="margin-left:7px">
                                        <input type="radio" class="chkForAll" name="Sessions" checked id="chkForAll" value="COMON">
                                        <label id="lblForAll" value="COMON" class="form-label">Common</label>
                                    </label>
                                    <label style="margin-left:2px">
                                        <input type="radio" class="chkDealer" name="Sessions" id="chkDealer" value="Dealer">
                                        <label id="lblDealer" value="Dealer" style="" class="form-label"> Dealer </label>
                                        @Html.DropDownList("ddlAllDealer", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlAllDealer, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlAllDealer" })
                                    </label>
                                </div>
                            </div>
                            <div class="makespace"></div>
                        </div>


                        <!--next-->
                        <div class="col-md-4" hidden>
                             <label class="form-label">Labour A/C</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-male"></i></span>
                                <input type="text" class="form-control" id="txtJobLabour" maxlength="150">

                            </div>
                            <div class="makespace"></div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:20px">
                        <ul class="nav nav-tabs nav-justified primary">
                            <li class="active">
                                <a href="#general" data-toggle="tab">
                                    <i></i> General
                                </a>
                            </li>
                            <li>
                                <a href="#inventory" data-toggle="tab">
                                    <i></i> Inventory
                                </a>
                            </li>
                          
                            <li>
                                <a href="#purchase" data-toggle="tab">
                                    <i></i> Purchase
                                </a>
                            </li>
                            <li>
                                <a href="#sales" data-toggle="tab">
                                    <i></i> Sales
                                </a>
                            </li>
                        </ul>

                        <div class="tab-content primary">
                            <div class="tab-pane fade in active" id="general">
                                <div class="row">
                                    <div class="col-md-4">
                                         <label class="form-label">Item Category</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            @Html.DropDownList("ddlItemCatDesc", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlItemCatDesc, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlItemCatDesc" })
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                         <label class="form-label">Product</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('Product')" style="color:blue;"></i></span>
                                            <input type="text" class="form-control" id="txtProdDesc" style="text-transform:uppercase" readonly>
                                            <input type="hidden" class="form-control" id="hdnProdCode" readonly hidden>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2">
                                         <label class="form-label">Unit</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            @Html.DropDownList("txtUnitDesc", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlUnit, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "txtUnitDesc" })
                                           @* <input type="text" class="form-control" id="txtUnitDesc" readonly>*@
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2" style="margin-top:10px">
                                         <label class="form-label">Pack Size</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtPackSize" value="0">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2" style="margin-top:10px">
                                         <label class="form-label">Part Type</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                            <select class="form-control m-bot15" id="ddlPartType">
                                                <option value="0">Select</option>
                                                <option value="Y">Lube</option>
                                                <option value="N">Spare</option>
                                            </select>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2" style="margin-top:10px">
                                         <label class="form-label">Liter</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtLiter" readonly maxlength="5">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2">
                                         <label class="form-label">Accessories</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                            <select class="form-control m-bot15" id="ddlAccessories">
                                                <option value="0">Select</option>
                                                <option value="Y">Yes</option>
                                                <option value="N">No</option>
                                            </select>
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-2">
                                         <label class="form-label">Unit of Sold</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtUnitOfSold">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>


                                    <div class="col-md-4">
                                         <label class="form-label">Source</label>
                                        <div class="box" style="height:60px">
                                            <label class="col-md-3">
                                                <input type="radio" class="chkSource" name="CheckBox" id="lblsLocal" value="Local">
                                                <label class="form-label" id="lblLocal" value="Local">Local</label>
                                            </label>
                                            <label class="col-md-3">
                                                <input type="radio" class="chkSource" name="CheckBox" checked id="lblsCKD" value="C.K.D">
                                                <label class="form-label" id="lblCKD" value="C.K.D">C.K.D</label>
                                            </label>
                                            <label class="col-md-3">
                                                <input type="radio" class="chkSource" name="CheckBox" id="lblsImported" value="Imported">
                                                <label class="form-label" id="lblImported" value="Imported"> Imported </label>
                                            </label>
                                            <label class="col-md-3">
                                                <input type="radio" class="chkDealers" name="CheckBox" id="lblsMarket" value="Market">
                                                <label class="form-label" id="lblMarket" value="Market"> Market </label>
                                            </label>
                                        </div>

                                    </div>
                                    <div class="col-md-6" style="margin-top:10px">
                                        <div class="box_toggle" style="height:72px;">

                                            <label class=" form-label col-md-4">
                                                <input id="chksaleitem" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label class="form-label" for="ctl00_Content_cbinventoryItem">  Sale Item</label>

                                            </label>
                                            <label class=" form-label col-md-4">
                                                <input id="chkinventoryitem" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label class="form-label" for="ctl00_Content_cbinventoryItem">  Inventory Item</label>

                                            </label>
                                            <label class=" form-label col-md-4">
                                                <input id="chkactive" type="checkbox" name="ctl00$Content$cbinventoryItem" /><label class="form-label" for="ctl00_Content_cbinventoryItem">  Active</label>

                                            </label>

                                        </div>

                                    </div>

                                </div>
                            </div>

                            <div class="tab-pane fade" id="inventory">
                                <div class="row">


                                    <div class="col-md-4">
                                         <label class="form-label">Min Level</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                            <input type="text" class="form-control" id="txtMinLevel" value="0">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                         <label class="form-label">Reorder Level</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                            <input type="text" class="form-control" id="txtReorderLevel" value="0">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                         <label class="form-label">War</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                            <input type="text" class="form-control" id="txtWar" value="0">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>


                                  

                                </div>
                            </div>
                           

                            <div class="tab-pane fade" id="purchase">

                            
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-md-2" style="margin-top:10px">
                                            <label class="form-label">Purchase Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                <input type="text" class="form-control" id="txtPurchaseRate">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">GST%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtGstPur">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">Further Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtFurtherTaxPur">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">Extra Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtExtraTaxPur">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4" style="margin-top:10px">
                                             <label class="form-label">Stock In A/C</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('AccountStockIn')" style="color:blue;"></i></span>
                                                <input type="text" class="form-control" id="txtStockIn">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Promotion Prod</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtPromotionProdPur">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">COGS A/C</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('AccountCOGS')" style="color:blue;"></i></span>
                                                <input type="text" class="form-control" id="txtCOGS">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>


                                    </div>

                                </div>
                                


                            </div>
                            <div class="tab-pane fade" id="sales">

                                <div class="row" style="margin-top:1rem;">

                                    <div class="col-md-12">
                                        <div class="col-md-2" style="margin-top:10px">
                                            <label class="form-label">Sales Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                <input type="text" class="form-control" id="txtSalesAmount">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                      
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">GST%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtGstSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">Further Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtFurtherTaxSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2" style="margin-top:10px">
                                             <label class="form-label">Extra Tax%</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                                <input type="text" class="form-control" id="txtExtraTaxSales">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>

                                        <div class="col-md-4" style="margin-top:10px">
                                             <label class="form-label">Stock Out A/C</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('AccountStockOut')" style="color:blue;"></i></span>
                                                <input type="text" class="form-control" id="txtStockOut">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <label class="form-label">Promotion Prod</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-tag"></i></span>
                                            <input type="text" class="form-control" id="txtPromotionProdSales">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                    <div class="col-md-4">
                                        <label class="form-label">Revenue A/C</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-search" onclick="ShowModal('AccountRevenue')" style="color:blue;"></i></span>
                                            <input type="text" class="form-control" id="txtRevenue">
                                        </div>
                                        <div class="makespace"></div>
                                    </div>
                                </div>

                            </div>
                            


                        </div>

                    </div>
                    <!--SAVE/Clear/Delete BUTTONS-->
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-md-4 pull-right">
                                @*onclick="SaveMasterEnquiry()"*@
                                <button type="button" class="btn btn-info" id="btnSave" style="margin-top:27px; width:85px; border-radius:0px;">Save</button>

                                <button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:27px; width:85px; border-radius:0px;">Delete</button>

                                <button type="reset" class="btn btn-default" id="btnClear" style="margin-top:27px; width:85px; border-radius:0px;">Clear</button>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </section>
        </div>
    </form>
    <div class="modal" id="section-confirm" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirm</h4>
                </div>
                <div class="modal-body">

                    <h5>Are you sure want to save ?</h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                    <button class="btn btn-success" type="button" onclick="SaveMasterEnquiry()">Save</button>
                </div>
            </div>
        </div>
    </div>
    <!-- modal end -->
    <div class="modal" id="section-confirm-Delete" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirm</h4>
                </div>
                <div class="modal-body">

                    <h5>Are you sure want to Delete ?</h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                    <button class="btn btn-danger" type="button" onclick="DeleteMasterEnquiry()">Delete</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Popup Message box modal start -->
    <div class="modal" id="section-message" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header" id="modalMessage">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Message</h4>
                </div>
                <div class="modal-body">

                    <h5 id="lblMessage"></h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" onclick="location" type="button">Close</button>


                </div>
            </div>
        </div>
    </div>
    <!-- Popup Message box modal End -->

    <div class="modal" tabindex="-1" id="JobModal" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="margin-top:60px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Close</button>
                    <h4 class="modal-title" id="modal123Label">Item</h4>
                </div>
                <div class="modal-body">
                 
                    <div class="form-group row">
                        <div id="dataList">
                            @Html.Partial("LookUp")
                        </div>
                </div>
                
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
        </div>
    @*Product Code Modal-> START*@
    <div class="modal" tabindex="-1" id="ProductModal" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="margin-top:60px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="modal123Label">Product Code</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group row">
                        <label for="exampleInputuname" class="col-md-2">Search :</label>
                        <div class="col-md-10">
                            @*<input type="text" id="txtEnquirySearch1" class="form-control" aria-controls="EnquiryTable" placeholder="Search . . . ">*@
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="table-responsive" style="height:300px;">
                            <table id="ProductTable" class="table table-striped table-hover responsive">
                                <thead>
                                    <tr>
                                        <th>Sr</th>
                                        <th>Dealer</th>
                                        <th>Product  Number</th>
                                        <th>Product  Description</th>
                                    </tr>
                                </thead>
                                <tbody style="font-size:12px; text-transform: uppercase"></tbody>
                            </table>
                        </div>
                    </div>
